<p>エラーメッセージ側 object_id: <%= resource.object_id %></p>
<p>resource.class: <%= resource.class.name %></p>
<p>resource.errors.full_messages: <%= resource.errors.full_messages.inspect %></p>
<p>resource.object_id: <%= resource.object_id %></p>
<% if resource && resource.errors.any? %>
  <div class="alert alert-danger">
    <h4 class="alert-heading">入力にエラーがあります:</h4>
    <ul>
      <% resource.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
    </ul>
  </div>
<% end %>