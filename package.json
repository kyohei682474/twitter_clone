{
  "name": "app",
  "private": "true",
  "dependencies": {
    "@hotwired/stimulus": "^3.2.1",
    "@hotwired/turbo-rails": "^7.2.4",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "5.3.3",
    "bootstrap-icons": "^1.10.3",
    "esbuild": "^0.17.0",
    "sass": "^1.57.1"
  },
  "scripts": {
    "build": "esbuild app/javascript/application.js --bundle --sourcemap --outdir=app/assets/builds --public-path=assets --resolve-extensions=.js,.jsx --loader:.js=js --node-path=node_modules",
    "build:css": "sass ./app/assets/stylesheets/application.bootstrap.scss:./app/assets/builds/application.css --no-source-map --load-path=node_modules"
  }
}
